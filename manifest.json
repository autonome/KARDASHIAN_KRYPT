{
  "manifest_version": 2,
  "name": "Kardashian Krypt",
  "version": "1.0",
  "description": "HIDE MESSAGES IN PICTURES OF THE KARDASHIANS",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "activeTab"
  ],
  "background": {
    "scripts": ["kardashiankrypt.js", "generatepng.js", "webtoolkit.base64.js"],
    "persistent": false
  },
  "browser_action": {
  }
}